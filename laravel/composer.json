        "moonshine/ru": "^1.0",
        "moonshine/tinymce": "^1.0",
        "socialiteproviders/vkontakte": "^5.0",
        "socialiteproviders/yandex": "^4.1",
        "spatie/laravel-sitemap": "^7.3",
        "tightenco/ziggy": "^2.0",
